        pages = {655--668},
        file = {PubMed Central Full Text PDF:/home/bougui/.mozilla/firefox/4kk9uau3.default/zotero/storage/QE9QTWRU/Baker et al. - 2012 - Gorgon and Pathwalking Macromolecular Modeling To.pdf:application/pdf}
}

