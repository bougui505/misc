        month = jan,
        year = {2017},
        pmid = {28104891},
        pages = {294--298},
        file = {Full Text PDF:/home/bougui/.mozilla/firefox/4kk9uau3.default/zotero/storage/NWRQKYZ4/Ovchinnikov et al. - 2017 - Protein structure determination using metagenome s.pdf:application/pdf;Snapshot:/home/bougui/.mozilla/firefox/4kk9uau3.default/zotero/storage/TDW2CGUB/294.html:text/html}
}

